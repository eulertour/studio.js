{
  "name": "@eulertour/studio",
  "description": "three.js code to use in Euler Studio",
  "author": "eulertour",
  "license": "ISC",
  "version": "1.0.30",
  "type": "module",
  "main": "./build/bundle.js",
  "typings": "./build/bundle.d.ts",
  "declaration": true,
  "declarationMap": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run buildSource && npm run buildApi",
    "buildSource": "rollup --config rollup.config.ts --configPlugin typescript",
    "buildApi": "npm run transpileJs && npm run extractApi && npm run parseJson",
    "transpileJs": "cd docs/tsc && tsc",
    "extractApi": "api-extractor run --config docs/apiExtractor/api-extractor.json --local --verbose",
    "parseJson": "cd docs/parser && tsc && node index.js ../apiExtractor/studio.api.json ../../build/api.json",
    "genTypes": "rollup --config rollup.three-types.config.ts --configPlugin typescript",
    "serve": "npm run build && cross-env PORT=8081 serve . --cors",
    "dev": "nodemon -w src -e js,ts --exec 'npm run serve'",
    "examples": "([ -d examples/build ] && ln -sfh $(pwd)/build/ examples/build); (cd examples/ && tsc); serve -S examples",
    "deploy-staging": "npm run deploy-build-staging && npm run deploy-source-staging && npm run deploy-examples-staging",
    "deploy-build-staging": "gsutil -m rsync -r build/ gs://euler-studio-staging.appspot.com/studio.js/build",
    "deploy-source-staging": "gsutil -m rsync -r src/ gs://euler-studio-staging.appspot.com/studio.js/src",
    "deploy-examples-staging": "gsutil -m rsync -r examples/ gs://euler-studio-staging.appspot.com/studio.js/examples"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/three": "^0.152.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "http-server": "^14.1.1",
    "nodemon": "^2.0.22",
    "rollup": "^3.20.2",
    "serve": "^14.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.42.1",
    "colors": "^1.4.0",
    "mathjax-full": "^3.2.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-ts": "^3.4.5",
    "rollup-plugin-typescript2": "^0.36.0",
    "three": "^0.153.0"
  }
}
