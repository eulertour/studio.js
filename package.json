{
  "name": "@eulertour/studio",
  "description": "three.js code to use in Euler Studio",
  "author": "eulertour",
  "license": "ISC",
  "version": "1.0.28",
  "type": "module",
  "main": "./build/bundle.js",
  "typings": "./build/bundle.d.ts",
  "declaration": true,
  "declarationMap": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "genTypes": "rollup --config rollup.three-types.config.ts --configPlugin typescript",
    "serve": "npm run build && cross-env PORT=8081 serve ./build --cors",
    "dev": "nodemon -w src -e js,ts --exec 'npm run serve'"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/three": "^0.152.0",
    "cross-env": "^7.0.3",
    "http-server": "^14.1.1",
    "nodemon": "^2.0.22",
    "rollup": "^3.20.2",
    "serve": "^14.2.0",
    "tslib": "^2.5.0",
    "typedoc": "^0.25.9",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.42.1",
    "@types/node": "^20.11.24",
    "mathjax-full": "^3.2.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-ts": "^3.4.5",
    "rollup-plugin-typescript2": "^0.36.0",
    "three": "^0.153.0"
  }
}
