    "@types/three": "^0.152.0",
    "cross-env": "^7.0.3",
    "http-server": "^14.1.1",
    "nodemon": "^2.0.22",
    "rollup": "^3.20.2",
    "serve": "^14.2.0",
    "tslib": "^2.5.0"
  },
  "dependencies": {
    "mathjax-full": "^3.2.2",
    "three": "^0.153.0"
  }
