{
  "name": "@eulertour/studio",
  "version": "1.0.33",
  "description": "EulerStudio graphics library",
  "author": "Devin Neal (devin@eulertour.com)",
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "bun run build:source; bun run build:api",
    "build:source": "tsc",
    "build:api": "bun run :extractApi && bun run :parseJson",
    ":extractApi": "api-extractor run --config docs/apiExtractor/api-extractor.json --local --verbose",
    ":parseJson": "node ./docs/parser/index.js docs/apiExtractor/studio.js.api.json ./dist/api.json",
    "deploy:emulator": "bun run :deploy -- --emulator",
    "deploy:staging": "bun run :deploy -- --staging",
    "deploy:production": "bun run :deploy -- --production",
    ":deploy": "bun run --cwd deploy :deploy"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.48.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/three": "^0.171.0",
    "ajv": "8",
    "colors": "^1.4.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "serve": "^14.2.4",
    "typescript": "5.4",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "mathjax-full": "^3.2.2",
    "three": "^0.171.0",
    "tslib": "^2.8.1"
  }
}
